{
  "name": "expressmongoserver",
  "version": "1.0.0",
  "description": "This directory contains two express servers: * AppServer.js - Combined Node/Express web server w/ Mongo Access * Server.js - Encapsulated Node/Express web server w/ Mongo Access",
  "main": "AppServer.js",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.4",
    "assert": "^2.1.0",
    "async": "^3.2.6",
    "body-parser": "^1.20.0",
    "bootstrap": "^5.3.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jquery": "^3.7.1",
    "mocha": "^10.8.2",
    "mongodb": "^4.5.0",
    "mongoose": "^5.7.5",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "openai": "^4.72.0",
    "q": "^2.0.3",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "start": "node AppServer.js",
    "nodemon": "nodemon",
    "build": "npm-run-all --serial tsc:build angular:build",
    "tsc:build": "tsc",
    "tsc:watch": "tsc --watch",
    "angular:build": "cd surveysage && npm run build",
    "db": "mongod -port 3000 -dbpath ./db",
    "dev": "npm-run-all --parallel tsc:watch nodemon"
  },
